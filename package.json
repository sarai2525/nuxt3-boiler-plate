{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "lint:script": "eslint --fix --ext \".js,.ts,.vue,.cjs,.mjs\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --write",
    "lint:style": "stylelint --fix --config stylelint.config.cjs"
  },
  "lint-staged": {
    "*.{.js,ts,vue,cjs,mjs}": "pnpm lint:script",
    "*.{md,json,yml,yaml}": "pnpm lint:prettier",
    "*.{vue,scss,css}": "pnpm lint:style"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@nuxtjs/eslint-module": "4.1.0",
    "@nuxtjs/stylelint-module": "5.1.0",
    "@typescript-eslint/parser": "6.18.0",
    "@unocss/reset": "0.58.3",
    "@vue/eslint-config-prettier": "8.0.0",
    "dayjs": "1.11.10",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.2",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "mobile-detect": "1.4.5",
    "nuxt": "3.9.1",
    "postcss-scss": "4.0.9",
    "prettier": "3.1.1",
    "sass": "1.69.7",
    "sass-loader": "13.3.3",
    "stylelint": "15.11.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "typescript": "5.3.3",
    "vee-validate": "4.12.4",
    "vue": "3.4.5",
    "vue-router": "4.2.5",
    "yup": "1.3.3"
  }
}
