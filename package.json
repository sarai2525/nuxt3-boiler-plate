{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nuxt build",
    "dev": "nuxt dev -o",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install && cp .env.example .env",
    "lint:script": "eslint --fix --ext \".js,.ts,.vue,.cjs,.mjs\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --write",
    "lint:style": "stylelint --fix --config stylelint.config.cjs",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{.js,ts,vue,cjs,mjs}": "pnpm lint:script",
    "*.{md,json,yml,yaml}": "pnpm lint:prettier",
    "*.{vue,scss,css}": "pnpm lint:style"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/image": "1.4.0",
    "@nuxt/test-utils": "3.12.0",
    "@nuxtjs/device": "3.1.1",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@nuxtjs/eslint-module": "4.1.0",
    "@nuxtjs/sitemap": "5.1.2",
    "@nuxtjs/stylelint-module": "5.2.0",
    "@nuxtjs/tailwindcss": "6.11.4",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "@unocss/reset": "0.58.8",
    "@vee-validate/nuxt": "4.12.6",
    "@vee-validate/yup": "4.12.6",
    "@vitest/coverage-v8": "1.4.0",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/test-utils": "2.4.5",
    "@zadigetvoltaire/nuxt-gtm": "0.0.13",
    "dayjs": "1.11.10",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "5.1.3",
    "happy-dom": "14.3.9",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "nuxt": "3.11.1",
    "nuxt-gtag": "2.0.5",
    "postcss-scss": "4.0.9",
    "prettier": "3.2.5",
    "sass": "1.72.0",
    "sass-loader": "14.1.1",
    "stylelint": "16.3.1",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-order": "6.0.4",
    "typescript": "5.4.3",
    "vee-validate": "4.12.6",
    "vitest": "1.4.0",
    "vue": "3.4.21",
    "vue-router": "4.3.0",
    "yup": "1.4.0",
    "zhead": "2.2.4"
  },
  "browserslist": [
    "> 2% in JP",
    "ios >= 15",
    "not dead"
  ]
}
